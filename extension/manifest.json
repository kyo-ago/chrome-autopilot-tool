{
    "name": "Chrome autopilot tool",
    "description": "Chrome autopilot tool",
    "manifest_version": 2,
    "default_locale": "en",
    "version": "0.0.0",
    "browser_action": {
        "default_title": "Chrome autopilot tool",
        "default_icon": "img/icon128.png"
    },
    "background": {
        "scripts": [
            "bower_components/eventemitter2/lib/eventemitter2.js",
            "src/_define.js",
            "src/background.js"
        ],
        "persistent": false
    },
    "permissions": [
        "<all_urls>",
        "storage",
        "tabs"
    ],
    "icons": {
        "48": "img/icon128.png"
    },
    "web_accessible_resources": [
        "/src/selenium-ide/iedoc-core.xml"
    ]
}