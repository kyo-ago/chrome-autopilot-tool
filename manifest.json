{
    "name": "Chrome autopilot tool",
    "description": "Chrome autopilot tool",
    "manifest_version": 2,
    "default_locale": "en",
    "version": "0.0.0",
    "browser_action": {
        "default_title" : "Chrome autopilot tool",
        "default_icon": "src/img/icon128.png",
        "default_popup": "src/html/popup.html"
    },
    "permissions": [
        "<all_urls>",
        "storage",
        "tabs"
    ],
    "icons": {
        "48": "src/img/icon128.png"
    },
    "web_accessible_resources": [
        "/src/js/selenium-ide/iedoc-core.xml"
    ]
}